
proc performAutoRegression { splunk_home login passwd index file field byfield response} {
    set tt [time {
        send "$splunk_home/bin/splunk search 'index=$index | xsPerformAutoRegression $field BY $byfield OUTPUT $file' -auth $login:$passwd -maxout 1 -header true -output table \r"

        expect {
            timeout { send_user "\nTEST FAIL - xsPerformAutoRegression - TIMEOUT" }
            "ERROR" { send_user "\nTEST FAIL - xsPerformAutoRegression - ERROR" }
            "Getinfo probe failed" { send_user "\nTEST FAIL - xsPerformAutoRegression - Getinfo probe" }
            "$response" { send_user "\nTEST PASS - xsPerformAutoRegression" }
        }
    }]
    send_user "\n$tt"
}


