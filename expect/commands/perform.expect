proc discoverTrend { splunk_home login passwd index field overfield byfield response} {
    set tt [time {
        set bylength [string length $byfield]
        if [ expr $bylength == 0] {
            send "$splunk_home/bin/splunk search 'index=$index | xsDiscoverTrend $field OVER $overfield ' -auth $login:$passwd -maxout 1 -header true -output table \r"
        } else {
            send "$splunk_home/bin/splunk search 'index=$index | xsDiscoverTrend $field OVER $overfield BY $byfield' -auth $login:$passwd -maxout 1 -header true -output table \r"
        }

        expect {
            timeout { send_user "\nTEST FAIL - xsDiscoverTrend($field OVER $overfield BY $byfield) - TIMEOUT" }
            "ERROR" { send_user "\nTEST FAIL - xsDiscoverTrend($field OVER $overfield BY $byfield) - ERROR" }
            "Getinfo probe failed" { send_user "\nTEST FAIL - xsDiscoverTrend($field OVER $overfield BY $byfield) - Getinfo probe" }
            "$response" { send_user "\nTEST PASS - xsDiscoverTrend($field OVER $overfield BY $byfield)" }
        }
    }]
    send_user "\n$tt"
}

proc predict { splunk_home login passwd index quantity field whenfield response} {
    set tt [time {
        send "$splunk_home/bin/splunk search 'index=$index | xsPredict $field WHEN $whenfield IS $quantity' -auth $login:$passwd -maxout 1 -header true -output table\r"

        expect {
            timeout { send_user "\nTEST FAIL - xsPredit($field WHEN $whenfield IS $quantity) - TIMEOUT" }
            "ERROR" { send_user "\nTEST FAIL - xsPredit($field WHEN $whenfield IS $quantity) - ERROR" }
            "Getinfo probe failed" { send_user "\nTEST FAIL - xsPredict($field WHEN $whenfield IS $quantity - Getinfo probe" }
            "$response" { send_user "\nTEST PASS - xsPredit($field WHEN $whenfield IS $quantity)" }
        }
    }]
    send_user "\n$tt"
}
