
stoptime=1200m

#eventcount=200
#
# Field 0 must be the time field
#
field.0.name=time
field.0.type=timestamp
field.0.format=%Y/%m/%d %H:%M:%S %Z
field.0.init=-180d@d
field.0.increment=20s
field.0.increment.variance=0.5

field.1.name=send_agent
field.1.type=string
field.1.file=./files/send_agent.csv
field.1.field=1

field.2.name=receive_country
field.2.type=string
field.2.file=./files/receive_country.csv
field.2.field=1

field.3.name=reference_number
field.3.type=int
field.3.init=10001
field.3.increment=1

field.4.name=txnAmt
field.4.type=float
field.4.min=20.00
field.4.dice=20*50
field.4.dice.weight=40/30/20/6/2
field.4.decimal=2
field.4.valid=1,2,3,4,5

field.5.name=txnAmt
field.5.type=float
field.5.min=50.00
field.5.dice=25*50
field.4.dice.weight=30/40/20/6/4
field.5.increment=1
field.5.valid=0,6

output.clock=time
output.file=./files/fraud.log
#output.header=time,UID,FirstName,LastName,Gender,Age,Height,Zip,Job,Salary
output.template=$time$ send_agent=$send_agent$ receive_country=$receive_country$ reference_number=$reference_number$ TxnAmt=$txnAmt$


